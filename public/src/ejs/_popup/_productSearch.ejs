                            <option value="107">WOMEN</option>
                            <option value="108">MEN</option>
                            <option value="109">ACCESSORIES</option>
                            <option value="301">FASHION&amp;ACC</option>
                            <option value="378">PLATFORM-L</option>
                            <option value="392">OÀLC</option>
                        </select>
                    </li>
                    <li class="product-search__item">
                        <select name="wirteOption" id="productSearch2" class="form__select js-multiselect" title="2차 카테고리 선택" disabled>
